<?xml version="1.0" encoding="utf-8"?>
<game title="Point and Click Adventure Game" width="1920" height="1080" fps="24" entryScene="1">
  <assets path="/assets">
    <player id="player" src="player_default.png" position="100"/>
    <sprite id="background" src="background.png"/>
    <sprite id="character" src="player_default.png"/>
    <sound id="boxSound" src="box01.mp3"/>
  </assets>
  <scenes>
    <scene sceneId="1">
      <character onClick ="boxSound" position="500, 1000"/>
      <background position="0, 20"/>
    </scene>
    <scene sceneId="2">
      <background position="200"/>
      <character onClick ="boxSound" position="500"/>
    </scene>
  </scenes>
</game>